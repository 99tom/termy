{
  "name": "termy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "start": "yarn build && electron dist/main.js",
    "build:native": "electron-build-env neon build native --release",
    "postinstall": "yarn build:native"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@monaco-editor/react": "^3.6.3",
    "@stitches/react": "0.0.3-canary.1",
    "monaco-editor": "^0.21.1",
    "react-portal": "^4.2.1",
    "react-syntax-highlighter": "^13.5.3",
    "react-use": "^15.3.4",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-react": "^0.59.0",
    "use-dark-mode": "^2.3.1",
    "uuid": "^8.3.0",
    "xterm": "^4.9.0",
    "xterm-addon-fit": "^0.4.0",
    "zustand": "^3.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-portal": "^4.0.2",
    "@types/react-syntax-highlighter": "^11.0.5",
    "@types/uuid": "^8.3.0",
    "electron": "^10.1.3",
    "electron-build-env": "^0.2.0",
    "electron-builder": "^22.8.1",
    "html-webpack-plugin": "^4.5.0",
    "neon-cli": "^0.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
